   "metadata": {},
   "outputs": [],
   "source": [
    "def diamond_shape():\n",
    "    rows = 5\n",
    "    for i in range(1, rows + 1):\n",
    "        print(\" \" * (rows - i) + \"* \" * i)\n",
    "    for i in range(rows - 1, 0, -1):\n",
    "        print(\" \" * (rows - i) + \"* \" * i)"
   ]
  },
